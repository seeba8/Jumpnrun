{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-classes-per-file": false,
        "no-reference": false,
        "no-console": false,
        "no-namespace": [
            false
        ],
        "curly": true,
        "eofline": true,
        "linebreak-style": [
            "LF"
        ],
        "indent": [
            4
        ],
        "semicolon": [
            "always"
        ]
    },
    "rulesDirectory": []
}